package day0131;

import java.util.Stack;

public class 괄호검사 {
	public static void main(String[] args) {
		String str = "((( )((((( )( )((( )( ))((( ))))))";
		Stack<Character> stack = new Stack<>();
		
		for(int i = 0; i<str.length(); ++i)
		{
			if(str.charAt(i) == '(')
				stack.push(str.charAt(i));
			else
				stack.pop();
		}
		
		if(stack.isEmpty())
			System.out.println("옳바름");
		else
			System.out.println("틀린");
	}
}
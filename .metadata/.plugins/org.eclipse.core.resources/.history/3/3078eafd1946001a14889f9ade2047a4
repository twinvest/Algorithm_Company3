package day0203;

import java.io.FileInputStream;
import java.util.Scanner;

public class 오목 {
	static int[][] map;
	static int N = 20;
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.setIn(new FileInputStream("Test54.txt"));
		Scanner sc = new Scanner(System.in);
		
		map = new int[N][N];
		for(int r=1; r<N; ++r)
		{			
			for(int c=1; c<N; ++c)
			{
				map[r][c] = sc.nextInt();
			}
		}
	}

}

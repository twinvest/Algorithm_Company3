package day0205;

import java.util.Scanner;

public class 원형큐 {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] queue = new int[4];
		int rear = -1;
		int front = -1;
		while(true) {
			System.out.println("1. enQueue\n2. deQueue\n3. size\n4. isEmpty\n0. End");
			int sel = sc.nextInt();
			if( sel == 0 )
				break;
			else if( sel == 1 ) {
				int num = sc.nextInt();
				rear +=1;
				queue[rear] = 1;
				//enqueue
			}
			else if( sel == 2 ) {
				//dequeue
				front += 1;
				System.out.println(queue[front]);
			}
			else if( sel == 3 ) {
				//size
			}
			else if( sel == 4 ) {
				//isEmpty
			}	
		}
	}
}

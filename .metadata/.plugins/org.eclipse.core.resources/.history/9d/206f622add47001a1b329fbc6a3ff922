package day0205;

import java.util.Scanner;

public class 중간평균값구하기 {

	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int testcase = scan.nextInt();
		for(int t=1; t<=testcase; ++t)
		{
			int max = -987654321;
			int min = 987654321;
			int[] arr = new int[10];
			for(int i=0; i<arr.length; ++i)
				arr[i] = scan.nextInt();
			for(int i=0; i<arr.length; ++i)
			{
				if(max < arr[i])
					max=arr[i];
			}
		}

	}

}

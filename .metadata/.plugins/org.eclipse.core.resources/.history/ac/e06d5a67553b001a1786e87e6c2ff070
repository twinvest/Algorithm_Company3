package day01;

public class WhileTest {
	public static void main(String[] args) {
		/*
		int dit = 1;
		
		while(dit <= 9) {
			System.out.println("2 * "+ dit + "=" +2*dit);
			dit++;
		}
		*/
		
		
		
		int dan = 2;
		while(dan <= 9) {
			if(dan == 8) continue;
			int dit = 1;
			dan ++;	
			if(dan==8) continue;
			
			while(dit <= 9) {
				System.out.println(dan + " * " + dit + "=" + dan *dit);
				dit ++;
				if(dit==4) continue;
			}
			
			
			
		}
	}
}

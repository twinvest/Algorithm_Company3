package day0205;

import java.util.LinkedList;
import java.util.Queue;

public class 마이쮸 {

	public static void main(String[] args) {
		int[] arr = new int[11];
		int i = 1;
		Queue<Integer> q = new LinkedList<>();
		int numOfmychu = 20;
		while(true)
		{
			q.add(i);
			arr[q.peek()] += ++arr[q.peek()];
			q.add(q.poll());
			++i;
		}
	}
}

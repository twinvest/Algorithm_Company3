package day0206;

public class 링크드리스트 {
	static class Node{
		int data;
		Node link;
	}
	public static void main(String[] args) {
		
		Node n1 = new Node();
		n1.data = 10;
		
		Node n2 = new Node();
		n2.data = 20;
		n1.link = n2;
		
		Node n3 = new Node();
		n3.data = 30;
		n2.link = n3;
		Node n4 = new Node();
		n4.data = 40;
		n3.link = n4;
		n2 = n3 =n4=null;
		
		Node head = n1;
		while(head != null)
		{
			
			if(head == null)
			{
				System.out.println(head.data);
				break;
			}
		}
		
	}
}

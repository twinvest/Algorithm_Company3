package day0203;

public class ProfileMain {

	public static void main(String[] args) {
		Profile p = new Profile();
		p.age = 23;
		p.name = "홍길동";
		p.gender = false;
		System.out.println(p.age+","+p.name+","+p.gender);
		
		Profile p2 = new Profile();
		p2.name = "팽수";
		p2.age = 10;
		p2.gender = true;
		System.out.println();
	}
}
